{
  "name": "halyard.js",
  "version": "1.3.1",
  "description": "Data import library for Qlik Analytics Platform",
  "author": "QlikTech International AB",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "coverage": "nyc report --reporter=text-lcov --temp-directory=./coverage/.nyc_output | coveralls",
    "test": "aw -c ./test/aw.config.json",
    "test:watch": "aw -c ./test/aw.config.json --mocha.timeout 5000 -w",
    "test:unit": "aw -c ./test/aw.config.json --glob \"./test/unit/**/*.spec.js\"",
    "test:unit:watch": "aw -c ./test/aw.config.json --glob \"./test/unit/**/*.spec.js\" -w",
    "test:integration": "aw -c ./test/aw.config.json --glob \"./test/integration/**/*.spec.js\" --mocha.timeout 5000",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/halyard.js"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/halyard.js",
  "devDependencies": {
    "after-work.js": "^4.0.0",
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "bluebird": "^3.4.6",
    "browser-sync": "^2.18.8",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "dev-ip": "^1.0.1",
    "enigma.js": "^2.2.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jsdoc": "^3.4.0",
    "rimraf": "^2.5.1",
    "rollup": "^0.52.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "webpack": "^3.10.0",
    "ws": "^3.3.3"
  },
  "dependencies": {}
}
